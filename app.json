{
  "expo": {
    "name": "tr-playground",
    "slug": "tr-playground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tr-playground-icon.png",
    "scheme": "trplayground",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tr-playground",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tr-playground-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.trplayground"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tr-playground-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tr-playground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d871f15f-4619-439e-b966-6557adbee051"
      }
    }
  }
}
